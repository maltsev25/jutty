{
  "name": "jutty",
  "version": "1.0.0",
  "dependencies": {
    "body-parser": "^1.16.1",
    "express": "^4.14.1",
    "grunt-cli": "^1.3.2",
    "node-pty": "^0.9.0",
    "optimist": "^0.6.1",
    "socket.io": "^2.3.0",
    "yalm": "^4.0.2"
  },
  "devDependencies": {
    "load-grunt-tasks": "^5.1.0",
    "grunt": "^1.1.0",
    "grunt-shell": "^3.0.1",
    "grunt-mkdir": "^1.0.0",
    "grunt-git": "^1.0.14",
    "grunt-contrib-clean": "^2.0.0",
    "nodemon": "^2.0.3"
  },
  "description": "Web-based ssh/telnet client, useful in environments where only http(s) is allowed",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/maltsev25/jutty.git"
  },
  "author": "maltsev25 (https://github.com/maltsev25)",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon app.js -p 3000"
  },
  "nodemonConfig": {
    "ext": "js",
    "ignore": [
      "bin",
      "public"
    ],
    "delay": 3,
    "execMap": {
      "ts": "node"
    }
  },
  "bugs": {
    "url": "https://github.com/maltsev25/jutty/issues"
  },
  "homepage": "https://github.com/maltsev25/jutty",
  "preferGlobal": true,
  "bin": {
    "wetty": "./bin/jutty.js"
  }
}
